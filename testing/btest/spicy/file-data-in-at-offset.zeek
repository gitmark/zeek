module SSH;

function get_file_handle(c: connection, is_orig: bool): string
	{
	return cat(c$uid);
	}

event zeek_init()
	{
	Analyzer::register_for_port(Analyzer::ANALYZER_SPICY_SSH, 22/tcp);
	Files::register_protocol(Analyzer::ANALYZER_SSH, [$get_file_handle=SSH::get_file_handle]); # use tag of replaced analyzer

	}

# @TEST-START-FILE ssh.spicy
module SSH;

import spicy;
import zeek;


public type Banner = unit {
    magic   : /SSH-/;
    version : /[^-]*/;
    dash    : /-/;
    software: /[^\r\n]*/;
};

# this is a bit of cheating...

on Banner::%done {
    local d: spicy::Base64Stream;
    local dec : bytes = spicy::base64_decode(d, b"");
    dec += spicy::base64_finish(d);
    local id = zeek::file_begin("application/x-x509-ca-cert");
    print id;
    zeek::file_data_in_at_offset(dec, 0);
    zeek::file_end();
}

# @TEST-END-FILE

# @TEST-START-FILE ssh-cond.evt

import zeek;

protocol analyzer spicy::SSH over TCP:
    parse with SSH::Banner,
    replaces SSH;

# @TEST-END-FILE
